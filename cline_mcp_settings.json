{
  "mcpServers": {
    "github.com/browserbase/mcp-server-browserbase": {
      "command": "node",
      "args": ["dist/index.js"],
      "cwd": "browserbase-mcp-server/browserbase",
      "env": {},
      "disabled": false,
      "autoApprove": []
    },
    "github.com/Garoth/wolframalpha-llm-mcp": {
      "command": "node",
      "args": ["build/index.js"],
      "cwd": "wolframalpha-llm-mcp",
      "env": {
        "WOLFRAM_LLM_APP_ID": "your-api-key-here"
      },
      "disabled": false,
      "autoApprove": [
        "ask_llm",
        "get_simple_answer",
        "validate_key"
      ]
    }
  }
}
